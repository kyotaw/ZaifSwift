# platform :ios, ‘9.0’
# Uncomment this line if you're using Swift
 use_frameworks!

source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '9.1'
use_frameworks!

target 'ZaifSwift' do
  pod 'CryptoSwift', '0.5'
  pod 'Alamofire', '~> 3.4'
  pod 'SwiftWebSocket'
  pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git'
end

target 'ZaifSwiftTests' do
  pod 'CryptoSwift', '0.5'
  pod 'Alamofire', '~> 3.4'
  pod 'SwiftWebSocket'
  pod 'SwiftyJSON', :git => 'https://github.com/SwiftyJSON/SwiftyJSON.git'
end
